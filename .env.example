# Development tooling environment variables
# Copy this file to .env and fill in your actual values

# OpenAI API key for AI development assistants (e.g., Voder)
OPENAI_API_KEY=sk-proj-your-key-here

# OpenAI model to use
OPENAI_MODEL=gpt-4o-mini

# Enable agentic mode for Voder
VODER_AGENTIC_MODE=true

# NPM authentication token for publishing (if needed for CI/CD testing)
#NPM_AUTH_TOKEN=npm_your-token-here
